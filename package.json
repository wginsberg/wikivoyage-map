{
  "name": "wikivoyage-app",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "/",
  "scripts": {
    "fetch-wiki-data": "curl -O https://dumps.wikimedia.org/enwikivoyage/latest/enwikivoyage-latest-pages-articles-multistream.xml.bz2 && bunzip2 enwikivoyage-latest-pages-articles-multistream.xml.bz2",
    "init-db": "rm -f data.db && sqlite3 data.db < scripts/init-db.sql",
    "build-nodes": "node scripts/build_nodes.js",
    "build-redirects": "node scripts/build_redirects.js",
    "build-edges": "node scripts/build_edges.js",
    "prune-edges": "node scripts/prune_edges.js",
    "build-world-json": "node scripts/world.js",
    "build-data": "npm run init-db && npm run build-nodes && npm run build-redirects && npm run build-edges && npm run prune-edges && npm run build-world-json",
    "start": "vite",
    "build": "tsc && vite build",
    "postbuild": "react-snap",
    "serve": "vite preview",
    "test": "react-app-rewired test"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.18",
    "better-sqlite3": "^9.6.0",
    "react-snap": "^1.23.0",
    "typescript": "^4.9.5",
    "xml-flow": "^1.0.4"
  },
  "dependencies": {
    "@vitejs/plugin-react-swc": "^3.6.0",
    "leaflet": "^1.8.0",
    "leaflet-doubletapdrag": "^0.1.1",
    "leaflet-doubletapdragzoom": "^0.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.0.2",
    "react-router-dom": "^6.21.3",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "web-vitals": "^3.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
