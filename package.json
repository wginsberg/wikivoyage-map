{
  "name": "where-u-going",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "/",
  "scripts": {
    "fetch-wiki-data": "curl -O https://dumps.wikimedia.org/enwikivoyage/latest/enwikivoyage-latest-pages-articles-multistream.xml.bz2 && bunzip2 enwikivoyage-latest-pages-articles-multistream.xml.bz2",
    "init-db": "rm -f data.db && sqlite3 data.db < scripts/init-db.sql",
    "build-nodes": "vite-node scripts/build_nodes.ts",
    "build-redirects": "vite-node scripts/build_redirects.ts",
    "build-edges": "vite-node scripts/build_edges.ts",
    "prune-edges": "vite-node scripts/prune_edges.ts",
    "prune-nodes": "vite-node scripts/prune_nodes.ts",
    "build-world-json": "vite-node scripts/world.ts",
    "build-data": "npm run init-db && npm run build-nodes && npm run build-redirects && npm run build-edges && npm run prune-edges && npm run prune-nodes && npm run build-world-json",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.10",
    "@types/xml-flow": "^1.0.4",
    "better-sqlite3": "^9.6.0",
    "vite-node": "^1.6.0",
    "vitest": "^1.6.0",
    "wtf_wikipedia": "^10.3.0",
    "xml-flow": "^1.0.4"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.12",
    "@types/node": "^20.12.8",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "leaflet": "^1.9.4",
    "protomaps-leaflet": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.0.2",
    "react-router-dom": "^6.21.3",
    "vite": "^5.2.11",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.17.2"
  }
}
